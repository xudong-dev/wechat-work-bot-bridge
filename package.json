{
  "name": "wechat-work-bot-bridge",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "license": "MIT",
  "engines": {
    "node": "12.x"
  },
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build && npm run build:client",
    "build:client": "npm install --prefix ./client && npm run build --prefix ./client",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@nestjs/bull": "0.0.3",
    "@nestjs/common": "^6.11.11",
    "@nestjs/config": "^0.2.4",
    "@nestjs/core": "^6.11.11",
    "@nestjs/graphql": "^6.6.2",
    "@nestjs/jwt": "^6.1.2",
    "@nestjs/passport": "^6.2.0",
    "@nestjs/platform-express": "^6.11.11",
    "@nestjs/serve-static": "^2.0.0",
    "@nestjs/typeorm": "^6.3.3",
    "apollo-server-express": "^2.11.0",
    "async-retry": "^1.3.1",
    "axios": "^0.19.2",
    "bcryptjs": "^2.4.3",
    "bullmq": "^1.8.2",
    "dotenv": "^8.2.0",
    "graphql": "^14.6.0",
    "graphql-request": "^1.8.2",
    "graphql-tools": "^4.0.7",
    "graphql-type-json": "^0.3.1",
    "ioredis": "^4.16.0",
    "lodash": "^4.17.15",
    "moment": "^2.24.0",
    "moment-timezone": "^0.5.28",
    "nestjs-pino": "^1.1.3",
    "numeral": "^2.0.6",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "pg": "^7.18.2",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^6.5.3",
    "source-map-support": "^0.5.16",
    "type-graphql": "^0.17.6",
    "typeorm": "^0.2.24",
    "typeorm-snake-naming-strategy": "^1.0.2",
    "vm2": "^3.8.4"
  },
  "devDependencies": {
    "@nestjs/cli": "^6.14.2",
    "@nestjs/schematics": "^6.9.4",
    "@nestjs/testing": "^6.11.11",
    "@types/async-retry": "^1.4.1",
    "@types/bcryptjs": "^2.4.2",
    "@types/express": "^4.17.3",
    "@types/graphql-type-json": "^0.3.2",
    "@types/jest": "^25.1.4",
    "@types/lodash": "^4.14.149",
    "@types/node": "^13.9.0",
    "@types/passport-jwt": "^3.0.3",
    "@types/supertest": "^2.0.8",
    "@typescript-eslint/eslint-plugin": "^2.23.0",
    "@typescript-eslint/parser": "^2.23.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-simple-import-sort": "^5.0.1",
    "jest": "^25.1.0",
    "prettier": "^1.19.1",
    "supertest": "^4.0.2",
    "ts-jest": "^25.2.1",
    "ts-loader": "^6.1.1",
    "ts-node": "^8.6.2",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^3.8.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "./coverage",
    "testEnvironment": "node"
  },
  "eslintConfig": {
    "parser": "@typescript-eslint/parser",
    "extends": [
      "airbnb-base",
      "plugin:import/typescript",
      "plugin:@typescript-eslint/recommended",
      "plugin:prettier/recommended",
      "prettier/@typescript-eslint"
    ],
    "plugins": [
      "@typescript-eslint",
      "simple-import-sort"
    ],
    "rules": {
      "no-console": "off",
      "simple-import-sort/sort": "error",
      "import/prefer-default-export": "off",
      "import/extensions": [
        "error",
        "always",
        {
          "js": "never",
          "ts": "never"
        }
      ]
    },
    "overrides": [
      {
        "files": [
          "*.ts"
        ],
        "rules": {
          "import/no-cycle": "off"
        }
      },
      {
        "files": [
          "*.controller.ts",
          "*.queue.ts",
          "*.resolver.ts",
          "*.service.ts"
        ],
        "rules": {
          "class-methods-use-this": "off"
        }
      }
    ]
  }
}
